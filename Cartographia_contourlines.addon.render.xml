<renderingStyle name="contourlines" addon="yes" depends="" version="1">
	<renderingProperty attr="baseAppMode" name="Default Rendering mode" description="Map optimization for respective User Profile based on base (parent) profile" type="string" possibleValues="default, car, bicycle, pedestrian, public_transport, boat, ski"/>

	<renderingProperty attr="contourColorScheme" name="Contour lines color scheme" description="Contour lines color scheme" type="string" possibleValues="white,yellow,green,orange,light_brown,brown,dark_brown,red,black,cartographia" defaultValueDescription="default"/>
	<renderingProperty attr="contourWidth" name="Contour lines width" description="Contour lines width" type="string" possibleValues="thin,medium,+,thick" defaultValueDescription="default"/>
	<renderingProperty attr="contourDensity" name="Contour lines density" description="Contour lines density" type="string" possibleValues="low,medium_w,+,high" defaultValueDescription="default"/>
	<renderingAttribute name="contourLineColor1">
		<case attrColorValue="#99d68c9d" />
	</renderingAttribute>
	<renderingAttribute name="chucontourLineTextColor">
		<case attrColorValue="$contourLineColor1" />
	</renderingAttribute>
	<renderingAttribute name="contourLineColor">
		<case contourColorScheme="white" attrColorValue="#44ffffff"/>
		<case contourColorScheme="yellow" attrColorValue="#44FFF29E"/>
		<case contourColorScheme="green" attrColorValue="#4400ff00"/>
		<case contourColorScheme="light_brown" attrColorValue="#449C5615">
			<apply_if noPolygons="true" attrColorValue="#FFEAD6"/>
			<apply_if minzoom="18" attrColorValue="#9C5615"/>
		</case>
		<case contourColorScheme="black" attrColorValue="#44000000">
			<apply_if noPolygons="true" attrColorValue="#999999"/>
			<apply_if minzoom="18" attrColorValue="#000000"/>
		</case>
		<case contourColorScheme="orange" attrColorValue="#44ff9f00">
			<apply_if noPolygons="true" attrColorValue="#FFBB4C"/>
			<apply_if minzoom="18" attrColorValue="#ff9f00"/>
		</case>
		<case contourColorScheme="cartographia" attrColorValue="$contourLineColor1">
			<apply_if noPolygons="true" attrColorValue="$contourLineColor1"/>
			<apply_if minzoom="18" attrColorValue="$contourLineColor1"/>
		</case>
		<case contourColorScheme="brown" attrColorValue="#44bd6819">
			<apply_if noPolygons="true" attrColorValue="#C39F7D"/>
			<apply_if minzoom="18" attrColorValue="#bd6819"/>
		</case>
		<case contourColorScheme="dark_brown" attrColorValue="#22522D0B">
			<apply_if noPolygons="true" attrColorValue="#37250B"/>
			<apply_if minzoom="18" attrColorValue="#522D0B"/>
		</case>
		<case contourColorScheme="red" attrColorValue="#44ff0000"/>
		<case attrColorValue="#44bd6819">
			<apply_if noPolygons="true" attrColorValue="#44FFEAD6"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="contourLineColor50m">
		<case contourColorScheme="white" attrColorValue="#44ffffff"/>
		<case contourColorScheme="yellow" attrColorValue="#44FFF29E"/>
		<case contourColorScheme="green" attrColorValue="#4400ff00"/>
		<case contourColorScheme="black" attrColorValue="#44000000">
			<apply_if noPolygons="true" attrColorValue="#999999"/>
			<apply_if minzoom="18" attrColorValue="#000000"/>
		</case>
		<case contourColorScheme="orange" attrColorValue="#44ff9f00">
			<apply_if noPolygons="true" attrColorValue="#FFBB4C"/>
			<apply_if minzoom="18" attrColorValue="#ff9f00"/>
		</case>
		<case contourColorScheme="cartographia" attrColorValue="$contourLineColor1">
			<apply_if noPolygons="true" attrColorValue="$contourLineColor1"/>
			<apply_if minzoom="18" attrColorValue="$contourLineColor1"/>
		</case>
		<case contourColorScheme="light_brown" attrColorValue="#aa9C5615">
			<apply_if noPolygons="true" attrColorValue="#FFEAD6"/>
			<apply_if minzoom="18" attrColorValue="#7b4f26"/>
		</case>
		<case contourColorScheme="brown" attrColorValue="#889C5615">
			<apply_if noPolygons="true" attrColorValue="#C39F7D"/>
			<apply_if minzoom="18" attrColorValue="#9C5615"/>
		</case>
		<case contourColorScheme="dark_brown" attrColorValue="#55522D0B">
			<apply_if noPolygons="true" attrColorValue="#37250B"/>
			<apply_if minzoom="18" attrColorValue="#522D0B"/>
		</case>
		<case contourColorScheme="red" attrColorValue="#44ff0000"/>
		<case attrColorValue="#889C5615">
			<apply_if noPolygons="true" attrColorValue="#9C5615"/>
		</case>
	</renderingAttribute>
	<renderingAttribute name="contourLineColor100m">
		<case contourColorScheme="white" attrColorValue="#44ffffff"/>
		<case contourColorScheme="yellow" attrColorValue="#44FFF29E"/>
		<case contourColorScheme="green" attrColorValue="#4400ff00"/>
		<case contourColorScheme="black" attrColorValue="#55000000">
			<apply_if noPolygons="true" attrColorValue="#999999"/>
			<apply_if minzoom="18" attrColorValue="#000000"/>
		</case>
		<case contourColorScheme="orange" attrColorValue="#55ff9f00">
			<apply_if noPolygons="true" attrColorValue="#FFBB4C"/>
			<apply_if minzoom="18" attrColorValue="#ff9f00"/>
		</case>
		<case contourColorScheme="cartographia" attrColorValue="$contourLineColor1">
			<apply_if noPolygons="true" attrColorValue="$contourLineColor1"/>
			<apply_if minzoom="18" attrColorValue="$contourLineColor1"/>
		</case>
		<case contourColorScheme="light_brown" attrColorValue="#aa9C5615">
			<apply_if noPolygons="true" attrColorValue="#FFEAD6"/>
			<apply_if minzoom="18" attrColorValue="#7b4f26"/>
		</case>
		<case contourColorScheme="brown" attrColorValue="#aa9C5615">
			<apply_if noPolygons="true" attrColorValue="#C39F7D"/>
			<apply_if minzoom="18" attrColorValue="#9C5615"/>
		</case>
		<case contourColorScheme="dark_brown" attrColorValue="#66522D0B">
			<apply_if noPolygons="true" attrColorValue="#37250B"/>
			<apply_if minzoom="18" attrColorValue="#522D0B"/>
		</case>
		<case contourColorScheme="red" attrColorValue="#44ff0000"/>
		<case attrColorValue="#aa9C5615">
			<apply_if noPolygons="true" attrColorValue="#9C5615"/>
		</case>
		<text>
			<switch tag="contour" value="elevation" nameTag="elevation" textOnPath="true" textBold="true" textOrder="255" textHaloColor="$chucontourLineTextColor" textHaloRadius="0" intersectionMargin="20">
				<case minzoom="13" contourLines="11"/>
				<case minzoom="13" contourLines="12"/>
				<case minzoom="13" contourLines="13"/>
				<case minzoom="13" contourLines=""/>
				<case minzoom="14" contourLines="14"/>
				<case minzoom="15" contourLines="15"/>
				<case minzoom="16" contourLines="16"/>
				<apply>
					<case minzoom="16" additional="contourtype=20m" textOrder="252" textBold="true" textColor="$chucontourLineTextColor" textSize="10" textMinDistance="170">
						<apply_if nightMode="true" textColor="#A3A3A3"/>
					</case>
					<switch minzoom="16">
						<case additional="contourtype=10m"/>
						<case additional="contourtype=40f"/>
						<apply textColor="$chucontourLineTextColor" textOrder="253" textBold="true" textSize="9" textMinDistance="180"/>
						<apply_if nightMode="true" textColor="#999999"/>
					</switch>
					<switch minzoom="13" maxzoom="13">
						<case additional="contourtype=100m" textSize="10"/>
						<case additional="contourtype=1000f" textSize="10"/>
						<case additional="contourtype=200f" textSize="10"/>
						<apply textColor="$chucontourLineTextColor" textOrder="250" textMinDistance="250"/>
						<apply_if nightMode="true" textColor="#BBBBBB"/>
					</switch>
					<switch minzoom="14">
						<case additional="contourtype=100m" textSize="11"/>
						<case additional="contourtype=1000f" textSize="11"/>
						<case additional="contourtype=200f" textSize="11"/>
						<apply textOnPath="false" textColor="$chucontourLineTextColor" textOrder="250" textMinDistance="250"/>
						<apply_if minzoom="16" textOnPath="true" textBold="true" textColor="$chucontourLineTextColor" textOrder="250" textMinDistance="150"/>
						<apply_if nightMode="true" textColor="#BBBBBB"/>
					</switch>
					<case minzoom="14" maxzoom="15" additional="contourtype=50m" textOnPath="false" textColor="$chucontourLineTextColor" textSize="11" textMinDistance="200" textOrder="251"/>
					<case minzoom="16" additional="contourtype=50m" textOnPath="true" textBold="true" textColor="$chucontourLineTextColor" textSize="11" textMinDistance="200" textOrder="160"/>
					<apply_if nightMode="true" textColor="#AAAAAA"/>
					<apply_if nightMode="true" textHaloRadius=":1" textHaloColor="#aa333333"/>
				</apply>
			</switch>
		</text>
		<line>
			<switch tag="contour" value="elevation">
				<case minzoom="11" contourLines="11"/>
				<case minzoom="12" contourLines="12"/>
				<case minzoom="13" contourLines="13"/>
				<case minzoom="13" contourLines=""/>
				<case minzoom="14" contourLines="14"/>
				<case minzoom="15" contourLines="15"/>
				<case minzoom="16" contourLines="16"/>
				<apply>
					<switch contourWidth="thin">
						<case additional="contourtype=10m" color="$contourLineColor">
							<case minzoom="12" maxzoom="13" strokeWidth="0.2"/>
							<case minzoom="14" strokeWidth="0.5"/>
						</case>
						<case additional="contourtype=20m" color="$contourLineColor">
							<case minzoom="11" maxzoom="13" strokeWidth="0.2"/>
							<case minzoom="14" strokeWidth="0.4"/>
						</case>
						<case additional="contourtype=50m" color="$contourLineColor50m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.2"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.4"/>
							<case minzoom="14" strokeWidth="0.6"/>
						</case>
						<case additional="contourtype=100m" color="$contourLineColor100m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.5"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.6"/>
							<case minzoom="14" strokeWidth="1"/>
						</case>
					</switch>
					<switch contourWidth="medium">
						<case additional="contourtype=10m" color="$contourLineColor">
							<case minzoom="12" maxzoom="13" strokeWidth="0.2"/>
							<case minzoom="14" strokeWidth="0.7"/>
						</case>
						<case additional="contourtype=20m" color="$contourLineColor">
							<case minzoom="11" maxzoom="11" strokeWidth="0.2"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.3"/>
							<case minzoom="14" strokeWidth="0.7"/>
						</case>
						<case additional="contourtype=50m" color="$contourLineColor50m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.4"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.6"/>
							<case minzoom="14" strokeWidth="1"/>
						</case>
						<case additional="contourtype=100m" color="$contourLineColor100m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.8"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.9"/>
							<case minzoom="14" strokeWidth="1.3"/>
						</case>
					</switch>
					<switch contourWidth="+">
						<case additional="contourtype=10m" color="$contourLineColor">
							<case minzoom="12" maxzoom="12" strokeWidth="0.3"/>
							<case minzoom="13" maxzoom="14" strokeWidth="0.5"/>
							<case minzoom="15" maxzoom="17" strokeWidth="0.9"/>
							<case minzoom="18" strokeWidth="1.5"/>
						</case>
						<case additional="contourtype=20m" color="$contourLineColor">
							<case minzoom="12" maxzoom="12" strokeWidth="0.3"/>
							<case minzoom="13" maxzoom="14" strokeWidth="0.5"/>
							<case minzoom="15" maxzoom="17" strokeWidth="0.9"/>
							<case minzoom="18" strokeWidth="1.5"/>
						</case>
						<case additional="contourtype=50m" color="$contourLineColor50m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.5"/>
							<case minzoom="12" maxzoom="12" strokeWidth="0.8"/>
							<case minzoom="13" maxzoom="14" strokeWidth="1"/>
							<case minzoom="15" maxzoom="17" strokeWidth="1.3"/>
							<case minzoom="18" strokeWidth="2.5"/>
						</case>
						<case additional="contourtype=100m" color="$contourLineColor100m">
							<case minzoom="11" maxzoom="11" strokeWidth="0.5"/>
							<case minzoom="12" maxzoom="12" strokeWidth="0.8"/>
							<case minzoom="13" maxzoom="14" strokeWidth="1"/>
							<case minzoom="15" maxzoom="17" strokeWidth="1.3"/>
							<case minzoom="18" strokeWidth="2.5"/>
						</case>
					</switch>
					<switch contourWidth="thick">
						<case additional="contourtype=10m" color="$contourLineColor">
							<case minzoom="12" maxzoom="13" strokeWidth="0.2"/>
							<case minzoom="14" maxzoom="17" strokeWidth="1.5"/>
							<case minzoom="18" strokeWidth="1.5"/>
						</case>
						<case additional="contourtype=20m" color="$contourLineColor">
							<case minzoom="11" maxzoom="11" strokeWidth="0.2"/>
							<case minzoom="12" maxzoom="13" strokeWidth="0.5"/>
							<case minzoom="14" maxzoom="17" strokeWidth="1"/>
							<case minzoom="18" strokeWidth="1"/>
						</case>
						<case additional="contourtype=50m" color="$contourLineColor50m">
							<case minzoom="12" maxzoom="13" strokeWidth="0.8"/>
							<case minzoom="14" maxzoom="17" strokeWidth="1.5"/>
							<case minzoom="18" strokeWidth="1.5"/>
						</case>
						<case additional="contourtype=100m" color="$contourLineColor100m">
							<case minzoom="11" maxzoom="11" strokeWidth="1.1"/>
							<case minzoom="12" maxzoom="13" strokeWidth="1.3"/>
							<case minzoom="14" strokeWidth="1.9"/>
						</case>
					</switch>
					<switch>
						<case minzoom="13" additional="contourtype=20f" color="$contourLineColor"/>
						<apply>
							<case contourWidth="thin" strokeWidth="0.3"/>
							<case contourWidth="medium" strokeWidth="0.6"/>
							<case contourWidth="+" strokeWidth="0.8"/>
							<case contourWidth="thick" strokeWidth="1.1"/>
							<case strokeWidth="0.3"/>
						</apply>
					</switch>
					<switch>
						<case minzoom="12" additional="contourtype=40f" color="$contourLineColor"/>
						<apply>
							<case contourWidth="thin" strokeWidth="0.5"/>
							<case contourWidth="medium" strokeWidth="1"/>
							<case contourWidth="+" strokeWidth="1.2"/>
							<case contourWidth="thick" strokeWidth="1.5"/>
							<case strokeWidth="0.5"/>
						</apply>
					</switch>
					<switch minzoom="11">
						<case additional="contourtype=200f" color="$contourLineColor100m"/>
						<apply>
							<case contourWidth="thin" strokeWidth="1"/>
							<case contourWidth="medium" strokeWidth="1.3"/>
							<case contourWidth="+" strokeWidth="1.4"/>
							<case contourWidth="thick" strokeWidth="1.9"/>
							<case strokeWidth="1"/>
						</apply>
					</switch>
					<switch minzoom="11">
						<case additional="contourtype=1000f" color="$contourLineColor100m"/>
						<apply>
							<case contourWidth="thin" strokeWidth="1.3"/>
							<case contourWidth="medium" strokeWidth="1.5"/>
							<case contourWidth="+" strokeWidth="2"/>
							<case contourWidth="thick" strokeWidth="2.1"/>
							<case strokeWidth="1.3"/>
						</apply>
					</switch>
				</apply>
			</switch>
		</line>
	</renderingAttribute>
</renderingStyle>
